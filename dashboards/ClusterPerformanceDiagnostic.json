{
  "endpoint": "localhost:9600",
  "gridOptions": {
    "rows": 12,
    "cols": 12
  },
  "graphs": {
    "tables": [
      {
        "queryParams" : {
          "metrics": "Disk_WaitTime",
          "aggregates": "avg",
          "dimensions": "DiskName",
          "sortBy": "Disk_WaitTime"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Average Disk Wait Time",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Disk_ServiceRate",
          "aggregates": "avg",
          "dimensions": "DiskName",
          "sortBy": "Disk_ServiceRate"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Average Disk Service Rate",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Heap_Used",
          "aggregates": "max",
          "dimensions": "MemType",
          "dimensionFilters": ["OldGen"],
          "sortBy": "Heap_Used"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 4,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Maximum Heap usage of Old GC",
          "keys": false,
          "fg": "white",
          "selectedFg": "white",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Heap_Used",
          "aggregates": "max",
          "dimensions": "MemType",
          "dimensionFilters": ["Eden"],
          "sortBy": "Heap_Used"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 4,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Maximum Heap usage of Young GC",
          "keys": false,
          "fg": "white",
          "selectedFg": "white",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Net_PacketDropRate4",
          "aggregates": "avg",
          "dimensions": "Direction",
          "sortBy": "Net_PacketDropRate4"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 8,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Packet Drop Rate (IPv4)",
          "keys": false,
          "fg": "cyan",
          "selectedFg": "cyan",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Net_PacketDropRate6",
          "aggregates": "avg",
          "dimensions": "Direction",
          "sortBy": "Net_PacketDropRate6"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 8,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Packet Drop Rate (IPv6)",
          "keys": false,
          "fg": "cyan",
          "selectedFg": "cyan",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "CB_EstimatedSize,CB_ConfiguredSize",
          "aggregates": "max,max",
          "dimensions": "CBType",
          "sortBy": "CB_EstimatedSize"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Circuit Breaker - Estimated and Configured Limits",
          "keys": false,
          "fg": "blue",
          "selectedFg": "blue",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Indexing_ThrottleTime",
          "aggregates": "avg",
          "dimensions": "IndexName",
          "sortBy": "Indexing_ThrottleTime"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Average Indexing Throttle Time",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      }
    ],
    "bars": [
      {
        "queryParams" : {
          "metrics": "CB_TrippedEvents",
          "aggregates": "sum",
          "dimensions": "CBType"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 6,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Circuit Breaker - Tripped Events (count)",
          "labelColor": "blue",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      }
    ],
    "lines": [
      {
        "queryParams" : {
          "metrics": "Master_PendingQueueSize",
          "aggregates": "max",
          "dimensions": ""
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 6,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Master Pending Tasks (count)",
          "showNthLabel": 3,
          "showLegend": true,
          "legend": { "width": 10 },
          "xAxis": ["0:00", "0:05", "0:10", "0:15", "0:20", "0:25", "0:30", "0:35", "0:40", "0:45", "0:50", "0:55"],
          "refreshInterval": 5000
        }
      }
    ]
  }
}