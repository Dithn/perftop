{
  "endpoint": "localhost:9600",
  "gridOptions": {
    "rows": 12,
    "cols": 12
  },
  "graphs": {
    "tables": [
      {
        "queryParams" : {
          "metrics": "wait",
          "aggregates": "sum",
          "dimensions": "name",
          "sortBy": "wait"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Disk Wait Time",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "srate",
          "aggregates": "sum",
          "dimensions": "name",
          "sortBy": "srate"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Disk Service Rate",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "used",
          "aggregates": "sum",
          "dimensions": "type",
          "dimensionFilters": ["OldGen"],
          "sortBy": "used"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 4,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Heap usage of Old GC",
          "keys": false,
          "fg": "white",
          "selectedFg": "white",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "used",
          "aggregates": "sum",
          "dimensions": "type",
          "dimensionFilters": ["Eden"],
          "sortBy": "used"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 4,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Heap usage of Young GC",
          "keys": false,
          "fg": "white",
          "selectedFg": "white",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "dropRate4",
          "aggregates": "sum",
          "dimensions": "direction",
          "sortBy": "dropRate4"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 8,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Packet Drop Rate (IPv4)",
          "keys": false,
          "fg": "cyan",
          "selectedFg": "cyan",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "dropRate6",
          "aggregates": "sum",
          "dimensions": "direction",
          "sortBy": "dropRate6"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 8,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Packet Drop Rate (IPv6)",
          "keys": false,
          "fg": "cyan",
          "selectedFg": "cyan",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "estimated,limitConfigured",
          "aggregates": "sum,sum",
          "dimensions": "type",
          "sortBy": "estimated"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Circuit Breaker - estimated & limit",
          "keys": false,
          "fg": "blue",
          "selectedFg": "blue",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      },
      {
        "queryParams" : {
          "metrics": "indexingThrottleTime",
          "aggregates": "avg",
          "dimensions": "indexName",
          "sortBy": "indexingThrottleTime"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Indexing Throttle Time",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 2000
        }
      }
    ],
    "bars": [
      {
        "queryParams" : {
          "metrics": "tripped",
          "aggregates": "sum",
          "dimensions": "type"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 6,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Circuit Breaker - tripped",
          "labelColor": "blue",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 2000
        }
      }
    ],
    "lines": [
      {
        "queryParams" : {
          "metrics": "pendingTasksCount",
          "aggregates": "sum",
          "dimensions": "n/a"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 6,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Pending Task Count",
          "showNthLabel": 5,
          "showLegend": true,
          "legend": { "width": 10 },
          "xAxis": ["0:00", "0:05", "0:10", "0:15", "0:20", "0:25", "0:30", "0:35", "0:40", "0:45", "0:50", "0:55"],
          "refreshInterval": 5000
        }
      }
    ]
  }
}