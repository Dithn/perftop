{
  "endpoint": "localhost:9600",
  "gridOptions": {
    "rows": 12,
    "cols": 12
  },
  "graphs": {
    "tables": [
      {
        "queryParams" : {
          "metrics": "ShardBulkDocs,ShardEvents",
          "aggregates": "sum,sum",
          "dimensions": "IndexName,Operation",
          "nodeName": "#nodeName",
          "sortBy": "ShardEvents"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 5
          },
          "label": "Shard Operation Metrics",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Cache_Request_Hit,Cache_Request_Miss",
          "aggregates": "sum,sum",
          "dimensions": "IndexName,ShardID",
          "nodeName": "#nodeName",
          "sortBy": "Cache_Request_Miss"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 5
          },
          "label": "Shard Request Cache Hit and Miss",
          "keys": false,
          "fg": "green",
          "selectedFg": "green",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Heap_Used",
          "aggregates": "max",
          "dimensions": "MemType",
          "dimensionFilters": ["Heap","OldGen","Eden","NonHeap","PermGen","Survivor"],
          "nodeName": "#nodeName",
          "sortBy": "Heap_Used"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 3
          },
          "label": "Maximum Heap Usage",
          "keys": false,
          "fg": "yellow",
          "selectedFg": "yellow",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "ThreadPool_QueueSize,ThreadPool_RejectedReqs,Thread_Blocked_Time",
          "aggregates": "max",
          "dimensions": "ThreadPoolType",
          "nodeName": "#nodeName",
          "sortBy": "ThreadPool_RejectedReqs"
        },
        "options": {
          "gridPosition": {
            "row": 3,
            "col": 5,
            "rowSpan": 3,
            "colSpan": 7
          },
          "label": "ThreadPool Metrics",
          "keys": false,
          "fg": "yellow",
          "selectedFg": "yellow",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Disk_Utilization",
          "aggregates": "avg",
          "dimensions": "DiskName",
          "nodeName": "#nodeName",
          "sortBy": "Disk_Utilization"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 9,
            "rowSpan": 3,
            "colSpan": 3
          },
          "label": "Disk Utilization",
          "keys": false,
          "fg": "yellow",
          "selectedFg": "yellow",
          "selectedBg": " ",
          "columnSpacing": 1,
          "refreshInterval": 5000
        }
      }
    ],
    "bars": [
      {
        "queryParams" : {
          "metrics": "Net_PacketDropRate4",
          "aggregates": "avg",
          "dimensions": "Direction",
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 8,
            "rowSpan": 3,
            "colSpan": 2
          },
          "label": "IPv4 Packet Drop Rate (packets/s)",
          "labelColor": "red",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "Net_PacketDropRate6",
          "aggregates": "avg",
          "dimensions": "Direction",
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 10,
            "rowSpan": 3,
            "colSpan": 2
          },
          "label": "IPv6 Packet Drop Rate (packets/s)",
          "labelColor": "yellow",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "GC_Collection_Event",
          "aggregates": "sum",
          "dimensions": "MemType",
          "dimensionFilters": ["totFullGC", "totYoungGC"],
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 0,
            "rowSpan": 3,
            "colSpan": 3
          },
          "label": "Total GC Collection Events (count)",
          "labelColor": "white",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams" : {
          "metrics": "GC_Collection_Time",
          "aggregates": "sum",
          "dimensions": "MemType",
          "dimensionFilters": ["totFullGC", "totYoungGC"],
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 3,
            "rowSpan": 3,
            "colSpan": 3
          },
          "label": "Total GC Collection Time (ms)",
          "labelColor": "white",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams": {
          "metrics": "CB_TrippedEvents",
          "aggregates": "sum",
          "dimensions": "CBType",
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 6,
            "col": 3,
            "rowSpan": 3,
            "colSpan": 5
          },
          "label": "Circuit Breaker - Tripped Events (count)",
          "labelColor": "blue",
          "barWidth": 20,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      },
      {
        "queryParams": {
          "metrics": "ShardEvents",
          "aggregates": "sum",
          "dimensions": "Operation",
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 0,
            "col": 5,
            "rowSpan": 3,
            "colSpan": 4
          },
          "label": "Shard Operation (count)",
          "labelColor": "blue",
          "barWidth": 12,
          "xOffset": 2,
          "maxHeight": 6,
          "refreshInterval": 5000
        }
      }
    ],
    "lines": [
      {
        "queryParams" : {
          "metrics": "CPU_Utilization",
          "aggregates": "sum",
          "dimensions": "Operation",
          "dimensionFilters": ["GC"],
          "nodeName": "#nodeName"
        },
        "options": {
          "gridPosition": {
            "row": 9,
            "col": 6,
            "rowSpan": 3,
            "colSpan": 6
          },
          "label": "Garbage Collection CPU (cores)",
          "showNthLabel": 3,
          "showLegend": true,
          "legend": { "width": 10 },
          "xAxis": ["0:00", "0:05", "0:10", "0:15", "0:20", "0:25", "0:30", "0:35", "0:40", "0:45", "0:50", "0:55"],
          "refreshInterval": 5000
        }
      }
    ]
  }
}